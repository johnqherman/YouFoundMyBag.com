    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^17.2.0",
    "html-webpack-plugin": "^5.5.3",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.1",
    "prettier-plugin-sql": "^0.18.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.3.6",
    "ts-loader": "^9.5.1",
    "tsx": "^4.6.2",
