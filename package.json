{
  "name": "youfoundmybag",
  "version": "1.0.0",
  "description": "Privacy-first return tag system",
  "private": true,
  "type": "module",
  "main": "dist/runtime/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/server.ts",
    "dev:frontend": "webpack serve --mode=development",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "webpack --mode=production",
    "start": "node dist/server.js",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "db:init": "psql ${DATABASE_URL:-postgresql://localhost:5432/youfoundmybag_dev} < database/schema.sql"
  },
  "dependencies": {
    "@atlaskit/icon": "^30.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/jsdom": "^27.0.0",
    "bullmq": "^5.66.5",
    "cors": "^2.8.5",
    "deep-email-validator": "^0.1.21",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.5",
    "helmet": "^8.1.0",
    "intl-tel-input": "^25.14.0",
    "ioredis": "^5.9.1",
    "jsdom": "^27.4.0",
    "libphonenumber-js": "^1.12.33",
    "mailgun.js": "^12.7.0",
    "nanoid": "^5.0.4",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.13.0",
    "twemoji": "^14.0.2",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.24",
