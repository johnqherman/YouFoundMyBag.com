{
  "name": "youfoundmybag",
  "version": "1.0.0",
  "description": "Privacy-first lost item recovery system",
  "private": true,
  "type": "module",
  "main": "dist/runtime/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/server.ts",
    "dev:frontend": "webpack serve --mode=development",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "webpack --mode=production",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "db:init": "psql ${DATABASE_URL:-postgresql://localhost:5432/youfoundmybag} < src/database/schema.sql"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "intl-tel-input": "^25.14.0",
    "nanoid": "^5.0.4",
    "nodemailer": "^7.0.12",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.5.3",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.1",
    "prettier-plugin-sql": "^0.18.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "ts-loader": "^9.5.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "keywords": [
    "lost",
    "found",
    "privacy",
    "qr",
    "recovery"
  ],
  "author": "",
  "license": "MIT"
}
